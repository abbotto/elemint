{
  "name": "@elemint/elemint",
  "description": "Tricks for the DOM",
  "version": "1.0.0",
  "author": "Jared Abbott",
  "license": "MIT",
  "entrypoint": "elemint.js",
  "private": false,
  "engines": {
    "node": "14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abbotto/elemint.git"
  },
  "scripts": {
    "build": "./ops/util/build.sh",
    "doc": "documentation build src/** -f html -o doc --theme node_modules/documentation-theme-light",
    "format": "pre-commit run format --all-files --verbose",
    "lint:js": "pre-commit run eslint --all-files --verbose",
    "lint:sh": "pre-commit run shellcheck --all-files --verbose",
    "test": "pre-commit run test --all-files --verbose"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "4.2.0",
    "documentation": "^13.1.0",
    "documentation-theme-light": "^1.1.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-disable": "^2.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-standard": "^4.1.0",
    "glob-concat": "^1.0.2",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "puppeteer": "^5.5.0",
    "uglify-es": "3.3.9"
  }
}
