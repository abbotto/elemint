{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "destination": "./doc",
        "readme": "./README.md",
        "encoding": "utf8",
        "template": "node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "path": "ink-docstrap",
        "theme": "elemint",
        "syntaxTheme": "dark",
        "navType": "vertical",
        "inverseNav": true,
        "linenums": true,
        "collapseSymbols": false,
        "dateFormat": "MMMM Do YYYY, h:mm:ss a",
        "systemName": "Elemint",
        "footer": "&nbsp;&nbsp;Elemint",
        "copyright": "2014-2018"
    }
}
